  "main": "./index.js",
  "scripts": {
    "bootstrap": "yarn && lerna bootstrap",
    "dev": "omni dev -P ./packages/mixin-jsbridge-sdk",
    "dev:demo": "omni dev -P ./packages/mixin-jsbridge-demo",
    "new": "omni new -P ./packages/mixin-jsbridge-sdk",
